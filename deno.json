{
  "name": "@cargo/parcel",
  "version": "0.0.5",
  "exports": {
    "./vNode": "./src/v-node/mod.ts",
    "./browser": "./src/platform/browser/mod.ts",
    "./server": "./src/platform/server/mod.ts",
    "./server/pack": "./src/platform/server/pack/mod.ts",
    "./state": "./src/state/mod.ts",
    "./hooks/lifecycle": "./src/hooks/lifecycle.ts",
    "./hooks/state": "./src/hooks/state.ts",
    "./jsx-runtime": "./src/jsx-runtime/mod.ts"
  },
  "publish": {
    "exclude": [
      "src/**/*.test.tsx",
      "src/**/*.test.ts",
      "old/*",
      "root.tsx",
      ".github/*",
      ".idea/*",
      ".nova/*"
    ]
  },
  "imports": {
    "@cargo/cargo": "jsr:@cargo/cargo@^0.0.8",
    "@std/assert": "jsr:@std/assert@^1.0.5",
    "@std/fs": "jsr:@std/fs@^1.0.3",
    "@std/path": "jsr:@std/path@^1.0.6",
    "@std/text": "jsr:@std/text@^1.0.9",
    "inspect/": "https://deno.land/x/cargo_inspect@0.2.0/",
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.11.0",
    "@esbuild": "npm:esbuild@0.20.2"
  },
  "tasks": {
    "check": "deno check src/**/*.tsx src/**/*.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@cargo/parcel",
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"]
  }
}
